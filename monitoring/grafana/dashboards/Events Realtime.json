          "format": "table",
          "rawSql": "WITH last_sim AS (SELECT run_id FROM capsim.simulation_runs ORDER BY start_time DESC LIMIT 1), start_end_values AS (SELECT DISTINCT ON (person_id, attribute_name) p.profession, person_id, attribute_name, FIRST_VALUE(old_value) OVER (PARTITION BY person_id, attribute_name ORDER BY change_timestamp ASC) as start_value, LAST_VALUE(new_value) OVER (PARTITION BY person_id, attribute_name ORDER BY change_timestamp ASC ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) as end_value FROM capsim.person_attribute_history pah JOIN capsim.persons p ON pah.person_id = p.id WHERE simulation_id = (SELECT run_id FROM last_sim)) SELECT profession, attribute_name, ROUND(((AVG(end_value) - AVG(start_value)) / NULLIF(AVG(start_value), 0) * 100)::NUMERIC, 1) as change_percent FROM start_end_values GROUP BY profession, attribute_name",
